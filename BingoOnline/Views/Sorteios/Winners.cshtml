@model IEnumerable<BingoOnline.Models.Sorteio>

@{
    ViewBag.Title = "Vencedores";
}

<h2>Winners</h2>
<h4>Bingo: @ViewBag.Bingo</h4>
<h4>Premio: @ViewBag.Premio</h4>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Usuario.UserName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.QuantidadeAcertos)
        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Usuario.UserName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.QuantidadeAcertos)
            </td>
        </tr>
    }

</table>

<div>
    @Html.ActionLink("Back to List", "Index", "OrdemSorteio", new { id = ViewBag.BingoId }, null)
</div>
